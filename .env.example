# AI API Keys
CLAUDE_API_KEY=sk-ant-your-key-here
GEMINI_API_KEY=your-gemini-key-here
OPENAI_API_KEY=your-openai-key-here
AI_PROVIDER=gemini

# AI Models (optional, will use defaults if not set)
# CLAUDE_MODEL=claude-sonnet-4-5-20250929
# GEMINI_MODEL=gemini-2.5-flash
# OPENAI_MODEL=gpt-4-turbo

# Rate Limits
MAX_REQUESTS_PER_MINUTE=10
MAX_REQUESTS_PER_DAY=500
MAX_API_CALLS_PER_HOUR=50

# Security
EXECUTION_TIMEOUT=30
MAX_MEMORY_MB=256
ALLOWED_MODULES=os,sys,json,math,datetime,re,itertools,collections,functools

# Core Features
ENABLE_WEB_SEARCH=true
ENABLE_AUTO_EVOLUTION=true
MAX_GENERATIONS=5
POPULATION_SIZE=3

# Redis
REDIS_URL=redis://redis:6379

# Database
DATABASE_URL=sqlite:///data/darwin.db

# ===================================
# Phase 2: AI Intelligence Enhancement
# ===================================

# Semantic Memory (ChromaDB)
ENABLE_SEMANTIC_MEMORY=true
CHROMA_PERSIST_DIRECTORY=./data/chroma

# Multi-Model Router
ENABLE_MULTI_MODEL=true
ROUTING_STRATEGY=tiered  # Options: performance, cost, speed, balanced, tiered

# ðŸ¦™ Ollama - Local LLM (FREE!)
# Runs locally for simple tasks - zero API costs!
OLLAMA_ENABLED=true
OLLAMA_URL=http://ollama:11434
OLLAMA_MODEL=llama3.2  # Options: llama3.2, mistral, codellama, phi3, etc.

# Web Research
ENABLE_WEB_RESEARCH=true
SERPAPI_API_KEY=your-serpapi-key-here
GITHUB_TOKEN=your-github-token-here

# Meta-Learning
ENABLE_META_LEARNING=true

# ===================================
# Phase 3: Autonomy & Personalities
# ===================================

# Multi-Agent System
ENABLE_MULTI_AGENT=true
DEFAULT_AGENT_MODE=auto  # Options: auto, random, best_performer
ENABLE_COLLABORATIVE_MODE=true
COLLABORATION_NUM_AGENTS=3

# Dream Mode (Autonomous Exploration)
ENABLE_DREAM_MODE=true
DREAM_IDLE_THRESHOLD_MINUTES=5
DREAM_CHECK_INTERVAL_SECONDS=60
DREAM_MAX_DURATION_MINUTES=30

# Code Poetry (Creative Self-Documentation)
ENABLE_CODE_POETRY=true
ENABLE_DAILY_DIARY=true

# Curiosity Engine (Self-Directed Learning)
ENABLE_CURIOSITY=true
CURIOSITY_LEVEL=0.7  # 0.0 to 1.0

# Auto-Benchmarking
ENABLE_AUTO_BENCHMARK=false

# Server
PORT=8000
